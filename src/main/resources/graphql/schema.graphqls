enum Gender {
    MALE, FEMALE
}

type UserListResult {
    id: ID!
    name: String
}

type UserResult {
    id: ID!
    name: String
    gender: Gender
}

type UserDeleteResult {
    id: ID!
}

type Query {
    getUsers: [UserListResult]
    getUser(id: ID!): UserResult
}

type Mutation {
    createUser(name: String!, gender: Gender!): UserResult
    updateUser(id: ID!, name: String!, gender: Gender!): UserResult
    deleteUser(id: ID!): UserDeleteResult
}