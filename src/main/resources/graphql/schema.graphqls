enum Gender {
    MALE, FEMALE
}

type User {
    id: ID!
    name: String!
    gender: Gender!
}

type UserDeleteResult {
    id: ID!
}

type Query {
    getUsers: [User]
    getUser(id: ID!): User
}

type Mutation {
    createUser(name: String!, gender: Gender!): User
    updateUser(id: ID!, name: String!, gender: Gender!): User
    deleteUser(id: ID!): UserDeleteResult
}